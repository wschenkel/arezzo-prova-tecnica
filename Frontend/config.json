{
	"destDev": "../Backend/assets/",
	"dest": "dist",
	"src": "./src/",
	"jsons": "src/json/**/**/*.json",
	"components": "materials/**/**/**/**/*.{hbs,md,yml}",
	"layouts": "pages/layouts/*.hbs",
	"layoutsIncludes": "pages/layouts/includes/*.hbs",
	"pages": "pages/**/**/**/*.hbs",
	"assemble": "**/*.{hbs,md,json,yml}",
	"ignoreLayouts": "/pages/+(layouts)/**",
	"data": "data/**/*.{json,yml}",
	"docs": "docs/**/*.md",
	"scripts": "./src/app.js",
	"styles": "src/sass/**/*.scss",
	"componentsScripts": "src/materials/components/**/**/**/*.js",
	"componentsStyles": "src/materials/components/**/**/**/*.scss",
	"pagesScripts": "src/pages/**/**/*.js",
	"pagesStyles": "src/pages/**/**/*.scss",
	"fonts": "src/assets/fonts/**/*",
	"images": "src/assets/img/**/**/*",
	"imagesSrc": "src/assets/img/",
	"sprites": "src/assets/img/sprite-src/**/*.png",
	"spriteDest": "src/assets/scss/",
	"sprites2x": "src/assets/img/sprite-src/**/*-2x.png"
}
